<h2>New gif</h2>

<%= form_for @gif, url: gifs_new_path do |f| %>
  <% @gif.errors.full_messages.each do |error| %>
    <ul>
      <div class="error"><%= error %></div>
  </ul>


  <% end %>

  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.submit "Create gifs" %>
<% end %>



